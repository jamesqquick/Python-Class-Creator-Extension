{
  "name": "python-class-generator",
  "displayName": "Python Class Generator",
  "description": "Will generate a Python class in the open project directory based on user input",
  "version": "0.0.1",
  "publisher": "jamesqquick",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createPyClass"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createPyClass",
        "title": "Create Python Class"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "eslint": "^5.13.0",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
